{
  "name": "@workspace-template/frontend",
  "type": "module",
  "scripts": {
    "dev": "node main.ts",
    "check": "pnpm lint:check && pnpm format:check",
    "fix": "pnpm lint:fix && pnpm format:fix",
    "lint": "oxlint --config .oxlintrc.json --tsconfig tsconfig.json --deny-warnings",
    "lint:check": "pnpm lint",
    "lint:fix": "pnpm lint --fix",
    "format": "oxfmt --config .oxfmtrc.json",
    "format:check": "pnpm format --check",
    "format:fix": "pnpm format --write",
    "typecheck": "tsc --noEmit",
    "prepare": "cd .. && husky frontend/.husky"
  },
  "devDependencies": {
    "husky": "9.1.7",
    "lint-staged": "16.2.7",
    "oxfmt": "0.33.0",
    "oxlint": "1.47.0",
    "typescript": "5.9.3"
  }
}
